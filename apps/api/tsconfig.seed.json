{
  "compilerOptions": {
    "outDir": "dist/seed",
    "rootDir": "prisma",
    "module": "Node16",
    "target": "ES2022",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "types": ["node"],
    "sourceMap": true,
    "allowJs": true
  },
  "include": ["prisma/**/*.ts"],
  "exclude": ["**/*.test.ts", "**/*.spec.ts", "node_modules"]
}
